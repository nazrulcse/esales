            <option value="product"> Product</option>
            <option value="service"> Service</option>
          </select>
        </div>
      </li>
      <li>
        <div>
          <input id="gh-btn" class="btn-prim" type="submit" value="Search">
        </div>
      </li>
    </ul>
  </div>
</div>

