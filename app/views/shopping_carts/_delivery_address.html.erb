<div class="panel panel-default">
  <div class="checkout-title"> Shipping Information </div>
  <div class="panel-body">
    <div class="col-lg-12 no-padding">
        <div class="form-group">
          <%= f.text_field :name, :class => 'form-control', :placeholder => 'First Name' %>
        </div>
        <div class="form-group">
          <%= f.text_field :mobile,  :class => 'form-control', :placeholder => 'Mobile Number' %>
        </div>
    </div>
    <div class="col-lg-12 no-padding">
      <div class="form-group">
        <%= f.text_area :address,  :class => 'form-control', :placeholder => 'Address' %>
      </div>
    </div>
    <div class="col-lg-12 no-padding">
        <div class="form-group">
          <%= f.text_field :city, :class => 'form-control', :placeholder => 'City' %>
        </div>
        <div class="form-group">
          <%= f.text_field :zip, :class => 'form-control', :placeholder => 'Zip' %>
        </div>
    </div>
    <!--<div class="actions pull-right">-->
    <!--<%#= submit_tag 'Continue', :class => 'btn btn-default' %>-->
    <!--</div>-->
  </div>
</div>